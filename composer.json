{
  "name": "oxidio/composer-plugin",
  "replace": {
    "oxid-esales/oxideshop-composer-plugin": "self.version"
  },
  "description": "This file is mean to be used to install OXID eShop compilation.",
  "type": "composer-plugin",
  "keywords": ["oxidEshop", "plugin"],
  "homepage": "https://www.oxid-esales.com/en/home.html",
  "license": [
    "GPL-3.0-only"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0",
    "composer-plugin-api": "^1.0||^2.0",
    "symfony/filesystem": "^3.0",
    "webmozart/glob": "^4.1",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~6",
    "mikey179/vfsstream": "^1.6",
    "squizlabs/php_codesniffer": "3.*",
    "composer/composer": "^1.0||^2.0"
  },
  "autoload": {
    "psr-4": {"OxidEsales\\ComposerPlugin\\": "src"}
  },
  "autoload-dev": {
    "psr-4": { "OxidEsales\\ComposerPlugin\\Tests\\": "tests/" }
  },
  "extra": {
    "branch-alias": {
      "dev-oxidio-2.x": "2.x-dev",
      "dev-oxidio-4.x": "4.x-dev",
      "dev-oxidio-6.x": "6.x-dev",
      "dev-oxidio-master": "7.x-dev"
    },
    "class": "OxidEsales\\ComposerPlugin\\Plugin"
  }
}
